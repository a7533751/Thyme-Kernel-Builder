[
  {
    "kernelSource": {
      "name": "Zf6-KernelSU-Proton",
      "repo": "https://github.com/a7533751/proton_zf6",
      "branch": "002v2",
      "device": "zenfone6",
      "defconfig": "kirin_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
      {
        "repo": "https://github.com/a7533751/proton-clang-f",
        "branch": "proton-clang-10",
        "name": "proton-clang",
        "binPath": ["bin"]
      },
      {
        "repo": "https://github.com/arter97/arm64-gcc",
        "branch": "master",
        "name": "arter97",
        "binPath": ["bin"]
      },
      {
        "repo": "https://github.com/arter97/arm32-gcc",
        "branch": "master",
        "name": "arter97-32",
        "binPath": ["bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "proton-clang/bin/clang",
      "externalCommand": {
        "CROSS_COMPILE": "arter97/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "arter97-32/bin/arm-linux-gnueabi-",
        "CLANG_TRIPLE": "arter97/bin/aarch64-linux-gnu-",
        "LD": "ld.lld",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        "READELF": "llvm-readelf",
        "OBJSIZE": "llvm-size",
        "STRIP": "llvm-strip",
        "LDGOLD": "aarch64-linux-gnu-ld.gold",
        "LLVM_AR": "llvm-ar",
        "LLVM_DIS": "llvm-dis"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/easterNday/AnyKernel3/",
      "branch": "thyme"
    }
  }
]
